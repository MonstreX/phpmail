{
    "name": "monstrex/phpmail",
    "description": "PHP Mail driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Ovdeyev",
            "email": "fido6080net@gmail.com",
            "homepage": "http://ss-dev.ru"
        }
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/monstrex/phpmail",
    "keywords": ["Laravel", "phpmail"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MonstreX\\PHPMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonstreX\\PHPMail\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MonstreX\\PHPMail\\PHPMailServiceProvider"
            ]
        }
    }
}
